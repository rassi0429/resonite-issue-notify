# Discord webhook URL for notifications
DISCORD_WEBHOOK_URL=

# GitHub personal access token (optional, but recommended to avoid rate limiting)
GITHUB_TOKEN=

# Comma-separated list of repositories to monitor (format: owner/repo)
REPOSITORIES=owner/repo1,owner/repo2

# Check interval in milliseconds (default: 300000 = 5 minutes)
CHECK_INTERVAL=300000

# OpenAI API key for Japanese translations (optional)
OPENAI_API_KEY=

# Misskey instance configuration (optional)
MISSKEY_INSTANCE_URL=
MISSKEY_TOKEN=
MISSKEY_CHANNEL_ID=